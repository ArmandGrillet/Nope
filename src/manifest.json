{
    "name": "Nope",
    "version": "1.0",
    "manifest_version": 2,
    "author": "Armand Grillet",
    "description": "Enhance your productivity",
    "homepage_url": "https://github.com/ArmandGrillet/Preview-for-Designer-News",

    "background": {
    	"scripts": [
            "js/background.js"
        ],
        "persistent": true
  	},
    "browser_action": {
	    "description": "Enhance your productivity"
	},
    "icons": {
    	"16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "permissions": [
        "contextMenus",
        "http://*/*",
        "https://*/*",
        "storage",
        "tabs"
    ],
    "web_accessible_resources": ["nope.html"]
}
